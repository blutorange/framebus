{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [ "./*" ],
      "src/*": ["./src/*"]
    },
    "types": ["node", "webdriverio", "expect-webdriverio", "@wdio/mocha-framework"]
  },
  "include": ["./src/**/*"],
  "exclude": ["./src/__tests__/**/*"]
}
